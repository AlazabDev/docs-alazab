{
  "name": "docs-alazab",
  "version": "1.0.0",
  "description": "Modern Dashboard for Alazab Document Repository",
  "license": "MIT",
  "author": "Alazab Team <info@alazab.com>",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "nuxt dev --host 127.0.0.1",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "dev:https": "nuxt dev --https",
    "dev:analyze": "ANALYZE=true nuxt build",
    "dev:debug": "NODE_OPTIONS='--inspect' nuxt dev",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx,.mjs",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.ts,.tsx,.mjs --fix",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\"",
    "type-check": "nuxt typecheck",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "security:audit": "pnpm audit --audit-level=high",
    "security:outdated": "pnpm outdated",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "clean": "rm -rf .nuxt .output node_modules coverage dist",
    "prepare": "husky install",
    "precommit": "pnpm run lint && pnpm run type-check",
    "prepush": "pnpm run test && pnpm run security:audit",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop alazab-dashboard",
    "pm2:restart": "pm2 restart alazab-dashboard",
    "pm2:logs": "pm2 logs alazab-dashboard --lines 100",
    "pm2:save": "pm2 save",
    "pm2:monitor": "pm2 monit",
    "pm2:setup": "pm2 startup && pm2 save",
    "deploy:staging": "nuxt build && ./scripts/deploy-staging.sh",
    "deploy:production": "nuxt build && ./scripts/deploy-production.sh"
  },
